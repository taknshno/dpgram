<% content_for(:title, 'お気に入り登録した画像') %>
<div class="profile-wrapper">
  <%= render 'profile' %>
  <div class="profile-tab">
    <div class="profile-tab-item">
      <i class="bi bi-border"></i><span>自分の投稿</span>
    </div>
    <div class="profile-tab-item tab-selected">
      <i class="bi bi-bookmark-heart"></i><span>お気に入り</span>
    </div>
  </div>
  <%= render 'layouts/pictures_index', { pictures: @favorites } %>
</div>
